{
  "name": "tb-learn",
  "private": true,
  "engines": {
    "node": "18.13.0",
    "npm": "~8.19.3"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lerna": "^6.6.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "sanity:dev": "lerna run dev",
    "sanity:deploy": "lerna run deploy",
    "gatsby:dev": "cd packages/web && gatsby develop",
    "gatsby:clean": "lerna run clean",
    "clean": "lerna clean -y",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "postinstall": "lerna bootstrap -- --legacy-peer-deps",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "build:digitalocean": "cd packages/web && gatsby clean && gatsby develop -H 0.0.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SearchEngineUnity/tb-learn.git"
